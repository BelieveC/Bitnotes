$(document).ready(function(){
	<% unless @examnotes.empty? %>
		var examnotes = $('<div class="recommended"><div class="recommended-grids"><% @examnotes.each do |examnote| %><div class="col-md-3 resent-grid recommended-grid"><div class="resent-grid-img recommended-grid-img"><% if examnote.eimages.count > 0 %><%= link_to image_tag(examnote.eimages.first.image.url(:small),alt: examnote.topic),examnote %><%else %><%= link_to image_tag("defaultcoverimage",alt: examnote.topic),examnote %><% end %><div class="time small-time" title="Pages Count"><p><%= examnote.eimages.count %></p></div><div class="clck small-clck" title="Pages"><span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span></div></div><div class="resent-grid-info recommended-grid-info video-info-grid"><h5><%= link_to examnote.topic,examnote,class:"title",title: examnote.topic%><br></h5><small>Submitted <strong> <%=time_ago_in_words(examnote.created_at) %> ago</strong></small><ul><li><p class="author author-info"><%= link_to examnote.user.phname,  controller:"users",action:"show",id:examnote.user_id,class:"author"%></p></li><li class="right-list"><p class="views views-info"><strong><%=examnote.views %></strong> views</p></li></ul></div></div><% end %><div class="clearfix"> </div></div></div>');
		$('.loadmoreexamnotediv').before(examnotes).fadeIn();
		$('.loadmoreexamnotediv').find('a').replaceWith('<%= link_to "Load More",loadmoreexamnotes_path(page: @examnotepageindex+1),remote: true,class:"btn btn-success" %>');
	<% end %>
});