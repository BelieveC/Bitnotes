$(document).ready(function(){
	<% unless @practicals.empty? %>
		var practicals = $('<div class="recommended"><div class="recommended-grids"><% @practicals.each do |practical| %><div class="col-md-3 resent-grid recommended-grid"><div class="resent-grid-img recommended-grid-img"><% if practical.pimages.count > 0 %><%= link_to image_tag(practical.pimages.first.image.url(:small),alt: practical.title),practical %><%else %><%= link_to image_tag("defaultcoverimage",alt: practical.title),practical %><% end %><div class="time small-time" title="Pages Count"><p><%= practical.pimages.count %></p></div><div class="clck small-clck" title="Pages"><span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span></div></div><div class="resent-grid-info recommended-grid-info video-info-grid"><h5><%= link_to practical.title,practical,class:"title",title: practical.title%><br></h5><small>Submitted <strong> <%=time_ago_in_words(practical.created_at) %> ago</strong></small><ul><li><p class="author author-info"><%= link_to practical.user.phname,  controller:"users",action:"show",id:practical.user_id,class:"author"%></p></li><li class="right-list"><p class="views views-info"><strong><%=practical.views %></strong> views</p></li></ul></div></div><% end %><div class="clearfix"> </div></div></div>');
		$('.loadmorepracticaldiv').before(practicals).fadeIn();
		$('.loadmorepracticaldiv').find('a').replaceWith('<%= link_to "Load More",loadmorepracticals_path(page: @practicalpageindex+1),remote: true,class:"loadmorepracticalbtn btn btn-success" %>');
	<% end %>
});